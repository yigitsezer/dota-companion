<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:id="@+id/progressshape" >
        <clip>
            <shape
                android:shape="rectangle">
                <gradient
                    android:angle="360"
                    android:startColor="#1E1E1E"
                    android:endColor="#3C464B"
                    />

                <stroke
                    android:color="#3C3C3C"
                    android:width="1dp" />
            </shape>
        </clip>
    </item>
</layer-list>